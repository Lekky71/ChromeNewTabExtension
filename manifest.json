{
    "name": "Smash New Tab",
    "version": "1.1",
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
    "description": "An new tab chrome extension with beautiful random HD wallpapers, current location weather widget.\n Popular website quicklink\n Google search",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "omnibox": {
        "keyword": "chrome new tab"
    },
    "browser_action": {
        "default_icon": "/images/logos/bookmark.png",
        "default_title": "Smash New Tab",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": [
            "/js/background.js",
          "/js/index.js"
        ]
    },
    "permissions": [
        "geolocation",
        "alarms",
        "bookmarks",
        "<all_urls>"
    ]


}